# LLM Routing Environment Variables
# Copy this file to .env and fill in your API keys

# DeepSeek API Key (primary - recommended)
# Get your key at: https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=

# OpenRouter API Key (optional - for fallback)
# Get your key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Groq API Key (optional)
# Get your key at: https://console.groq.com/keys
GROQ_API_KEY=

# LiteLLM Master Key - CHANGE THIS for non-local deployments!
# Generate a secure key for production use
LITELLM_MASTER_KEY=sk-aios-litellm-default

# Logging level (DEBUG, INFO, WARNING, ERROR)
LITELLM_LOG_LEVEL=INFO
