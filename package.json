{
  "name": "aios-fullstack",
  "version": "1.0.0-rc.6",
  "description": "AIOS-FULLSTACK: AI-Orchestrated System for Full Stack Development - Modern cross-platform CLI with interactive wizard",
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "bin": {
    "aios": "bin/aios-fullstack.js",
    "aios-fullstack": "bin/aios-fullstack.js"
  },
  "workspaces": [
    "aios-core",
    "memory",
    "security",
    "performance",
    "telemetry"
  ],
  "files": [
    "bin/",
    "install/",
    "tools/",
    "templates/",
    "aios-core/",
    "common/",
    "expansion-packs/",
    "memory/",
    "security/",
    "performance/",
    "telemetry/",
    "index.js",
    "index.esm.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node tools/package-builder.js",
    "build:agents": "node tools/cli.js build --agents-only",
    "build:teams": "node tools/cli.js build --teams-only",
    "build:packages": "node tools/package-builder.js",
    "list:agents": "node tools/cli.js list:agents",
    "validate": "node tools/cli.js validate",
    "install:aios": "node tools/installer/bin/aios.js install",
    "setup:github": "node tools/setup-github-cli.js",
    "setup": "npm run setup:github && npm run install:aios",
    "format": "prettier --write \"**/*.md\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "publish:dry-run": "node tools/npm-publish.js --dry-run",
    "publish:preview": "node tools/npm-publish.js --tag preview",
    "publish:stable": "node tools/npm-publish.js --tag latest",
    "publish:interactive": "node tools/npm-publish.js --interactive",
    "version:patch": "node tools/version-bump.js patch",
    "version:minor": "node tools/version-bump.js minor",
    "version:major": "node tools/version-bump.js major",
    "version:expansion": "node tools/bump-expansion-version.js",
    "version:expansion:set": "node tools/update-expansion-version.js",
    "version:all": "node tools/bump-all-versions.js",
    "version:all:minor": "node tools/bump-all-versions.js minor",
    "version:all:major": "node tools/bump-all-versions.js major",
    "version:all:patch": "node tools/bump-all-versions.js patch",
    "version:expansion:all": "node tools/bump-all-versions.js",
    "version:expansion:all:minor": "node tools/bump-all-versions.js minor",
    "version:expansion:all:major": "node tools/bump-all-versions.js major",
    "version:expansion:all:patch": "node tools/bump-all-versions.js patch",
    "release": "semantic-release",
    "release:test": "semantic-release --dry-run --no-ci || echo 'Config test complete - authentication errors are expected locally'",
    "prepare": "echo 'Skipping husky - not needed for NPM publish'"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@kayvan/markdown-tree-parser": "^1.5.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "inquirer": "^8.2.6",
    "isolated-vm": "^5.0.4",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "picocolors": "^1.1.1",
    "semver": "^7.7.2",
    "validator": "^13.15.15",
    "yaml": "^2.8.1"
  },
  "keywords": [
    "ai",
    "aios",
    "agile",
    "agents",
    "orchestrator",
    "fullstack",
    "development",
    "cli",
    "cross-platform",
    "interactive",
    "wizard",
    "modern-ux",
    "vite-style",
    "automation"
  ],
  "author": "AIOS Fullstack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allfluenceinc/aios-fullstack.git"
  },
  "bugs": {
    "url": "https://github.com/allfluenceinc/aios-fullstack/issues"
  },
  "homepage": "https://github.com/allfluenceinc/aios-fullstack#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^30.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.1.1",
    "prettier": "^3.5.3",
    "semantic-release": "^22.0.0",
    "yaml-lint": "^1.7.0"
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write"
    ]
  }
}
